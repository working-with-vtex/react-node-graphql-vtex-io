{
  "extends": ["tslint-config-airbnb"],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts", "coverage/lcov-report/*.js"]
  },
  "rules": {
    "interface-name": [true, "never-prefix"],
    "jsx-no-lambda": false,
    "import-name": false,
    "max-line-length": [true, 120],
    "variable-name": false,
    "trailing-comma": false
  }
}
